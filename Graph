#include <bits/stdc++.h>
using namespace std;
#define int long long 
#define endl "\n"
#define pb push_back
const int N=1e5;
int par[N];
int size[N];
//DSU

void make(int n){
	par[n]=n;
	size[n]=1;
}

int find(int v){
	if(par[v]==v){
		return v;
	}
	return par[v]=find(par[v]);
}

void Union(int a,int b){
	a=find(a);
	b=find(b);
	if(a!=b){
		if(size[a]<size[b]){
			swap(a,b);
		}
		par[b]=a;
		size[a]+=size[b];
	}
}

int32_t main(){
	int n,e;
	cin>>n>>e;
	for(int i=0;i<e;i++){
		int x,y;
		cin>>x>>y;
		make(x);
		make(y);
	}
}
